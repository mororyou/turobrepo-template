    "@tailwindcss/postcss": "^4.1.5",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "tailwindcss": "^4.1.5"
  }
